<%= render partial: 'navbar/navbar' %>
<div class='bookindex'>
  <h1>You are what you read</h1>
  <% @books.each do |book| %>
    <div><%= image_tag url_for(book.image) %></div>
    <div>Title: <%= book.title %></div>
    <div>Author: <%= book.author %></div>
    <% if current_user %>
      <div><%= link_to 'Edit', edit_book_path(book) %></div>
      <div><%= link_to 'Delete', book_path(book), method: 'delete' %></div>
    <% end %>
  <% end %>
</div>